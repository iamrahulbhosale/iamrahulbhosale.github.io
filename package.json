{
  "name": "react-app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "analyze":
      "npm-run-all clean profile && webpack-bundle-analyzer stats.json build/public -s gzip",
    "build-client": "webpack --config tools/webpack.config.client.js",
    "build-prod": "NODE_ENV=production npm-run-all build gzip-size",
    "build-server": "webpack --config tools/webpack.config.server.js",
    "build": "npm-run-all clean copy build-client build-server",
    "clean": "rm -rf ./build && mkdir -p build/views build/public",
    "copy-public": "cp -r ./src/public ./build/",
    "copy-views": "cp -r ./src/server/views ./build/",
    "copy-watch-public": "cpx \"./src/public/**/*.*\" ./build/public/ --watch",
    "copy-watch-views": "cpx ./src/server/views/**/*.* ./build/views/ --watch",
    "copy-watch": "npm-run-all --parallel copy-watch-views copy-watch-public",
    "copy": "npm-run-all copy-views copy-public",
    "deploy": "npm-run-all pm2-delete pm2-start",
    "dev-server": "nodemon build/server.bundle.js",
    "generate-pages": "DEBUG=react-app:* node tools/scripts/generate-pages",
    "gzip-size": "node tools/scripts/gzip-size",
    "pm2-delete": "pm2 delete react-app",
    "pm2-start":
      "NODE_ENV=production DEBUG=react-app:* pm2 start build/server.bundle.js --name react-app",
    "postinstall": "npm run build-prod",
    "profile":
      "NODE_ENV=production webpack --config tools/webpack.config.client.js --display normal --profile --json > stats.json",
    "server":
      "DEBUG=react-app:* node -r source-map-support/register build/server.bundle.js",
    "start": "npm run server",
    "watch-client":
      "webpack-dev-server --config tools/webpack.config.client.js",
    "watch-server": "webpack --watch --config tools/webpack.config.server.js",
    "watch":
      "npm-run-all clean build-client --parallel copy-watch watch-client watch-server"
  },
  "private": true,
  "dependencies": {
    "autoprefixer": "8.1.0",
    "axis": "^1.0.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "body-parser": "^1.18.2",
    "chalk": "2.3.2",
    "classnames": "^2.2.5",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.10",
    "dotenv": "^5.0.1",
    "easy-table": "^1.1.1",
    "eslint": "4.18.2",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "1.1.11",
    "fs-extra": "^5.0.0",
    "gzip-size-cli": "^2.1.0",
    "history": "^4.7.2",
    "jquery": "^3.3.1",
    "morgan": "^1.9.0",
    "nib": "^1.1.2",
    "nodemon": "^1.17.1",
    "npm-run-all": "^4.1.2",
    "popper.js": "^1.12.9",
    "postcss": "6.0.19",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.1",
    "pretty-error": "^2.1.1",
    "prop-types": "15.6.1",
    "pug": "2.0.0",
    "purify-css": "^1.2.5",
    "react": "^16.2.0",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "rupture": "^0.7.1",
    "source-map-support": "^0.5.3",
    "style-loader": "^0.20.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.0.1",
    "webpack": "4.1.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "2.0.10",
    "webpack-manifest-plugin": "^2.0.0-rc.2",
    "webpack-node-externals": "^1.6.0",
    "workbox-webpack-plugin": "^2.1.3"
  }
}
